<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qinweizhao.ware.mapper.WmsPurchaseDetailMapper">

    <resultMap type="WmsPurchaseDetail" id="WmsPurchaseDetailResult">
        <result property="id" column="id"/>
        <result property="purchaseId" column="purchase_id"/>
        <result property="skuId" column="sku_id"/>
        <result property="skuNum" column="sku_num"/>
        <result property="skuPrice" column="sku_price"/>
        <result property="wareId" column="ware_id"/>
        <result property="status" column="status"/>
        <result property="createBy" column="create_by"/>
        <result property="updateBy" column="update_by"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
        <result property="remark" column="remark"/>
    </resultMap>

    <sql id="selectWmsPurchaseDetailVo">
        select id,
               purchase_id,
               sku_id,
               sku_num,
               sku_price,
               ware_id,
               status,
               create_by,
               update_by,
               create_time,
               update_time,
               remark
        from wms_purchase_detail
    </sql>

    <select id="selectWmsPurchaseDetailList" parameterType="WmsPurchaseDetail" resultMap="WmsPurchaseDetailResult">
        <include refid="selectWmsPurchaseDetailVo"/>
        <where>
            <if test="purchaseId != null ">and purchase_id = #{purchaseId}</if>
            <if test="skuId != null ">and sku_id = #{skuId}</if>
            <if test="skuNum != null ">and sku_num = #{skuNum}</if>
            <if test="skuPrice != null ">and sku_price = #{skuPrice}</if>
            <if test="wareId != null ">and ware_id = #{wareId}</if>
            <if test="status != null ">and status = #{status}</if>
        </where>
    </select>

    <select id="selectWmsPurchaseDetailById" parameterType="Long" resultMap="WmsPurchaseDetailResult">
        <include refid="selectWmsPurchaseDetailVo"/>
        where id = #{id}
    </select>

    <insert id="insertWmsPurchaseDetail" parameterType="WmsPurchaseDetail" useGeneratedKeys="true" keyProperty="id">
        insert into wms_purchase_detail
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="purchaseId != null">purchase_id,</if>
            <if test="skuId != null">sku_id,</if>
            <if test="skuNum != null">sku_num,</if>
            <if test="skuPrice != null">sku_price,</if>
            <if test="wareId != null">ware_id,</if>
            <if test="status != null">status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="purchaseId != null">#{purchaseId},</if>
            <if test="skuId != null">#{skuId},</if>
            <if test="skuNum != null">#{skuNum},</if>
            <if test="skuPrice != null">#{skuPrice},</if>
            <if test="wareId != null">#{wareId},</if>
            <if test="status != null">#{status},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
        </trim>
    </insert>

    <update id="updateWmsPurchaseDetail" parameterType="WmsPurchaseDetail">
        update wms_purchase_detail
        <trim prefix="SET" suffixOverrides=",">
            <if test="purchaseId != null">purchase_id = #{purchaseId},</if>
            <if test="skuId != null">sku_id = #{skuId},</if>
            <if test="skuNum != null">sku_num = #{skuNum},</if>
            <if test="skuPrice != null">sku_price = #{skuPrice},</if>
            <if test="wareId != null">ware_id = #{wareId},</if>
            <if test="status != null">status = #{status},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteWmsPurchaseDetailById" parameterType="Long">
        delete
        from wms_purchase_detail
        where id = #{id}
    </delete>

    <delete id="deleteWmsPurchaseDetailByIds" parameterType="String">
        delete from wms_purchase_detail where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>